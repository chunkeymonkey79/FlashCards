      <Label fx:id="welcomeLabel" layoutX="96.0" layoutY="30.0" text="Welcome, User!" />
      <Label layoutX="96.0" layoutY="66.0" text="Select a Subject" />
      <Button layoutX="72.0" layoutY="621.0" mnemonicParsing="false" onAction="#createNewSubjectBtn" prefHeight="31.0" prefWidth="152.0" text="Create new Subject" />
      <Separator layoutX="309.0" layoutY="41.0" orientation="VERTICAL" prefHeight="632.0" prefWidth="6.0" />
      <ListView fx:id="listView" layoutX="49.0" layoutY="101.0" prefHeight="428.0" prefWidth="200.0" />
      <Button layoutX="72.0" layoutY="544.0" mnemonicParsing="false" onAction="#addFlashCardBtn" prefHeight="31.0" prefWidth="152.0" text="Add FlashCard" />
      <Button layoutX="72.0" layoutY="582.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="152.0" text="Remove FlashCard" />
